<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @street.name %>
</p>

<p>
  <strong>Image:</strong>
  <%= @street.image %>
</p>

<table>
  <thead>
    <tr>
      <th>Pronunciations</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if @street.voicemails.present? %>
      <% @street.voicemails.each do |voicemail| %>
        <tr>
          <td><%= link_to "Listen", voicemail.url %></td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_street_path(@street) %> |
<%= link_to 'Back', streets_path %>
